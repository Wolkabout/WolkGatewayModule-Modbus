#!/usr/bin/make -f
DH_VERBOSE = 1

%:
	dh $@ --builddirectory=out
override_dh_auto_configure:
    dh_auto_configure -- -DCMAKE_INSTALL_PREFIX=/opt/wolkabout/modbus-module/
override_dh_auto_clean:
override_dh_auto_test:
override_dh_shlibdeps:
	dh_shlibdeps -l$(shell pwd)/out/lib -l$(shell pwd)/debian/wolkgateway/opt/wolkabout/modbus-module/lib
